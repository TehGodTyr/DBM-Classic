package-as: DBM-Azeroth

externals:
  DBM-Azeroth: https://github.com/DeadlyBossMods/DBM-Unified
  DBM-Azeroth/Libs/CallbackHandler-1.0: https://repos.wowace.com/wow/callbackhandler/trunk/CallbackHandler-1.0
  DBM-Azeroth/Libs/LibChatAnims: https://repos.wowace.com/wow/libchatanims/trunk/LibChatAnims
  DBM-Azeroth/Libs/LibDBIcon-1.0: https://repos.wowace.com/wow/libdbicon-1-0/trunk/LibDBIcon-1.0
  DBM-Azeroth/Libs/LibDurability: https://repos.wowace.com/wow/libdurability/trunk/LibDurability
  DBM-Azeroth/Libs/LibLatency: https://repos.wowace.com/wow/liblatency/trunk/LibLatency
